<h1>Welcome, <%= User.find(session[:user_id]).username %>. Here are all the ideas.</h1>
<% @users.each do |user| %>
  <h4>All <%= user.username %>'s ideas:</h4><br>
  <% user.ideas.each do |idea| %>
    <h2>idea title: <%= idea.title %></h2>
    <%= link_to "see this idea", admin_idea_path(idea) %><br>
  <br>
  <% end %>
<% end %>
